{
  "name": "food-delivery-backend",
  "version": "1.0.0",
  "description": "Advanced food delivery backend with ML capabilities",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "postbuild": "prisma generate",
    "prisma:generate": "node use-postgresql.js && prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "node use-postgresql.js && prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "seed": "ts-node src/database/seed.ts",
    "railway:setup": "prisma generate && prisma migrate deploy"
  },
  "keywords": ["food-delivery", "ml", "api"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "socket.io": "^4.8.1",
    "unsplash-js": "^7.0.19",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "nodemon": "^3.1.7",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}

